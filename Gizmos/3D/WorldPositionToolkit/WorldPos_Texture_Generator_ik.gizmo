 addUserKnob {26 "" +STARTLINE}
 addUserKnob {26 credits l "" +STARTLINE T "v001 Â© Ivan Kokov. Yelow Mountain Studio. Tianjin. 2013"}
}
 Input {
  inputs 0
  name InputPointPos
  selected true
  xpos 14
  ypos 5
 }
set N64e269c0 [stack 0]
